set number
set mouse=a
set numberwidth=1
set clipboard=unnamed
syntax on
set showcmd
set ruler
set cursorline
set encoding=utf-8
set showmatch
set sw=2
set relativenumber

so ~/.config/nvim/plugins.vim
so ~/.config/nvim/maps.vim

set background=dark
let g:gruvbox_material_background='hard'
colorscheme gruvbox-material
let g:airline_theme='tomorrow'
